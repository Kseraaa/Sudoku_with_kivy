<MenuScreen>:
    BoxLayout:
        orientation: 'vertical'
        Button:
            text:'Start Game'
            on_press: root.manager.current = 'play'
        Button:
            text:'Quit'
            on_press: app.stop()

<SudokuScreen>:
    BoxLayout:
        orientation: 'vertical'
        Label:
            id : names
            text:'Sudoku game to play'
